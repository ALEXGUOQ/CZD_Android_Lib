<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TabButton">
        <attr name="srcOn" format="reference" />
        <attr name="srcOff" format="reference" />
        <attr name="backgroundOn" format="reference|color" />
        <attr name="backgroundOff" format="reference|color" />
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textOnColor" format="reference|color" />
        <attr name="textOffColor" format="reference|color" />
        <attr name="orientation">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>

</resources>